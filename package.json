{
  "name": "blog",
  "version": "1.0.0",
  "private": true,
  "description": "A blog to share stuff I find interesting or helpful and experiment with web technologies.",
  "scripts": {
    "dev": "run-p start typesafe-i18n",
    "start": "astro dev",
    "check": "prettier --check src && eslint . && astro check && tsc --noEmit",
    "fmt": "prettier --write src",
    "build": "astro build",
    "preview": "astro preview",
    "test": "playwright test",
    "generate-posts-images": "node ./scripts/generate-posts-images.mjs",
    "generate-devicons": "svgr --icon 1em --typescript --jsx-runtime automatic --index-template ./src/assets/svgs/devicons/index-template.js --svgo-config ./src/assets/svgs/devicons/svgo.config.js --out-dir ./src/components/devicons src/assets/svgs/devicons",
    "postinstall": "npx patch-package && npm run generate-devicons && npx playwright install && npx astro sync && npx typesafe-i18n --no-watch",
    "prepare": "husky install",
    "typesafe-i18n": "typesafe-i18n"
  },
  "dependencies": {
    "@astro-community/astro-embed-youtube": "^0.5.6",
    "@astrojs/check": "^0.2.1",
    "@astrojs/mdx": "^1.1.2",
    "@astrojs/react": "^3.0.3",
    "@astrojs/rss": "^3.0.0",
    "@astrojs/sitemap": "^3.0.1",
    "@astrojs/tailwind": "^5.0.2",
    "@codesandbox/sandpack-client": "^2.7.1",
    "@codesandbox/sandpack-react": "^2.8.0",
    "@codesandbox/sandpack-themes": "^2.0.21",
    "@floating-ui/dom": "^1.2.7",
    "@fontsource/merriweather": "^4.5.14",
    "@fontsource/source-sans-pro": "^5.0.1",
    "@playwright/test": "^1.38.1",
    "@svgr/cli": "^8.1.0",
    "@types/lodash": "^4.14.194",
    "@types/md5": "^2.3.2",
    "@types/mustache": "^4.2.2",
    "@types/node-fetch": "^2.6.3",
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "a11y-dialog": "^7.5.2",
    "astro": "^3.5.2",
    "astro-robots-txt": "^0.3.12",
    "astro-seo": "^0.6.1",
    "astro-toc": "^0.1.3",
    "autoprefixer": "^10.4.14",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "eslint": "^8.40.0",
    "eslint-mdx": "^2.1.0",
    "eslint-plugin-astro": "^0.21.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-mdx": "^2.1.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-react": "^7.32.2",
    "hast-util-from-html": "^1.0.2",
    "hast-util-select": "^6.0.2",
    "hast-util-to-html": "^9.0.0",
    "hast-util-to-string": "^2.0.0",
    "hastscript": "^7.2.0",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "lucide-react": "^0.293.0",
    "md5": "^2.3.0",
    "mdast-util-to-string": "^3.2.0",
    "node-fetch": "^2.6.9",
    "npm-run-all": "^4.1.5",
    "outdent": "^0.8.0",
    "patch-package": "^6.5.1",
    "postcss": "^8.4.23",
    "postcss-cli": "^8.3.1",
    "postcss-functions": "^4.0.2",
    "postcss-import": "^14.1.0",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.7.2",
