    "react": "^18.2.0",
    "react-compare-slider": "^2.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-katex": "^6.0.3",
    "rehype-slug": "^5.1.0",
    "remark-directive": "^2.0.1",
    "remark-math": "^5.1.1",
    "shiki": "^0.14.5",
    "shiki-twoslash": "^3.1.2",
    "start-server-and-test": "^1.15.4",
    "tailwind-merge": "^1.12.0",
    "tailwindcss": "^3.3.2",
    "typesafe-i18n": "^5.24.3",
    "typescript": "^5.2.2",
    "zod": "^3.21.4"
  }
}
